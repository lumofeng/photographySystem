ndex.vue","sourceRoot":"src/views/ruoyi-shoot/shoot","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <el-form :model=\"queryParams\" ref=\"queryForm\" size=\"small\" :inline=\"true\" v-show=\"showSearch\" label-width=\"68px\">\n      <el-form-item label=\"主题\" prop=\"titile\">\n        <el-input\n          v-model=\"queryParams.titile\"\n          placeholder=\"请输入主题\"\n          clearable\n          @keyup.enter.native=\"handleQuery\"\n        />\n      </el-form-item>\n      <el-form-item label=\"拍摄名称\" prop=\"shootName\">\n        <el-input\n          v-model=\"queryParams.shootName\"\n          placeholder=\"请输入拍摄名称\"\n          clearable\n          @keyup.enter.native=\"handleQuery\"\n        />\n      </el-form-item>\n      <el-form-item label=\"描述\" prop=\"description\">\n        <el-input\n          v-model=\"queryParams.description\"\n          placeholder=\"请输入描述\"\n          clearable\n          @keyup.enter.native=\"handleQuery\"\n        />\n      </el-form-item>\n      <el-form-item label=\"拍摄设备\" prop=\"shootEquipment\">\n        <el-input\n          v-model=\"queryParams.shootEquipment\"\n          placeholder=\"请输入拍摄设备\"\n          clearable\n          @keyup.enter.native=\"handleQuery\"\n        />\n      </el-form-item>\n      <el-form-item label=\"拍摄地点\" prop=\"shootLocation\">\n        <el-input\n          v-model=\"queryParams.shootLocation\"\n          placeholder=\"请输入拍摄地点\"\n          clearable\n          @keyup.enter.native=\"handleQuery\"\n        />\n      </el-form-item>\n      <el-form-item label=\"是否删除\" prop=\"isDelete\">\n        <el-input\n          v-model=\"queryParams.isDelete\"\n          placeholder=\"请输入是否删除\"\n          clearable\n          @keyup.enter.native=\"handleQuery\"\n        />\n      </el-form-item>\n      <el-form-item label=\"拍摄日期\" prop=\"shootDate\">\n        <el-date-picker clearable\n          v-model=\"queryParams.shootDate\"\n          type=\"date\"\n          value-format=\"yyyy-MM-dd\"\n          placeholder=\"请选择拍摄日期\">\n        </el-date-picker>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"handleQuery\">搜索</el-button>\n        <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetQuery\">重置</el-button>\n      </el-form-item>\n    </el-form>\n\n    <el-row :gutter=\"10\" class=\"mb8\">\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"primary\"\n          plain\n          icon=\"el-icon-plus\"\n          size=\"mini\"\n          @click=\"handleAdd\"\n          v-hasPermi=\"['shoot:ruoyi-shoot:add']\"\n        >新增</el-button>\n      </el-col>\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"success\"\n          plain\n          icon=\"el-icon-edit\"\n          size=\"mini\"\n          :disabled=\"single\"\n          @click=\"handleUpdate\"\n          v-hasPermi=\"['shoot:ruoyi-shoot:edit']\"\n        >修改</el-button>\n      </el-col>\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"danger\"\n          plain\n          icon=\"el-icon-delete\"\n          size=\"mini\"\n          :disabled=\"multiple\"\n          @click=\"handleDelete\"\n          v-hasPermi=\"['shoot:ruoyi-shoot:remove']\"\n        >删除</el-button>\n      </el-col>\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"warning\"\n          plain\n          icon=\"el-icon-download\"\n          size=\"mini\"\n          @click=\"handleExport\"\n          v-hasPermi=\"['shoot:ruoyi-shoot:export']\"\n        >导出</el-button>\n      </el-col>\n      <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar>\n    </el-row>\n\n    <el-table v-loading=\"loading\" :data=\"ruoyi-shootList\" @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\n      <el-table-column label=\"id\" align=\"center\" prop=\"photoId\" />\n      <el-table-column label=\"主题\" align=\"center\" prop=\"titile\" />\n      <el-table-column label=\"拍摄名称\" align=\"center\" prop=\"shootName\" />\n      <el-table-column label=\"描述\" align=\"center\" prop=\"description\" />\n      <el-table-column label=\"拍摄类型\" align=\"center\" prop=\"shootType\" />\n      <el-table-column label=\"拍摄设备\" align=\"center\" prop=\"shootEquipment\" />\n      <el-table-column label=\"拍摄地点\" align=\"center\" prop=\"shootLocation\" />\n      <el-table-column label=\"是否删除\" align=\"center\" prop=\"isDelete\" />\n      <el-table-column label=\"备注\" align=\"center\" prop=\"remark\" />\n      <el-table-column label=\"拍摄日期\" align=\"center\" prop=\"shootDate\" width=\"180\">\n        <template slot-scope=\"scope\">\n          <span>{{ parseTime(scope.row.shootDate, '{y}-{m}-{d}') }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\n        <template slot-scope=\"scope\">\n          <el-button\n            size=\"mini\"\n            type=\"text\"\n            icon=\"el-icon-edit\"\n            @click=\"handleUpdate(scope.row)\"\n            v-hasPermi=\"['shoot:ruoyi-shoot:edit']\"\n          >修改</el-button>\n          <el-button\n            size=\"mini\"\n            type=\"text\"\n            icon=\"el-icon-delete\"\n            @click=\"handleDelete(scope.row)\"\n            v-hasPermi=\"['shoot:ruoyi-shoot:remove']\"\n          >删除</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination\n      v-show=\"total>0\"\n      :total=\"total\"\n      :page.sync=\"queryParams.pageNum\"\n      :limit.sync=\"queryParams.pageSize\"\n      @pagination=\"getList\"\n    />\n\n    <!-- 添加或修改拍摄管理对话框 -->\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\n        <el-form-item label=\"主题\" prop=\"titile\">\n          <el-input v-model=\"form.titile\" placeholder=\"请输入主题\" />\n        </el-form-item>\n        <el-form-item label=\"拍摄名称\" prop=\"shootName\">\n          <el-input v-model=\"form.shootName\" placeholder=\"请输入拍摄名称\" />\n        </el-form-item>\n        <el-form-item label=\"描述\" prop=\"description\">\n          <el-input v-model=\"form.description\" placeholder=\"请输入描述\" />\n        </el-form-item>\n        <el-form-item label=\"拍摄设备\" prop=\"shootEquipment\">\n          <el-input v-model=\"form.shootEquipment\" placeholder=\"请输入拍摄设备\" />\n        </el-form-item>\n        <el-form-item label=\"拍摄地点\" prop=\"shootLocation\">\n          <el-input v-model=\"form.shootLocation\" placeholder=\"请输入拍摄地点\" />\n        </el-form-item>\n        <el-form-item label=\"是否删除\" prop=\"isDelete\">\n          <el-input v-model=\"form.isDelete\" placeholder=\"请输入是否删除\" />\n        </el-form-item>\n        <el-form-item label=\"备注\" prop=\"remark\">\n          <el-input v-model=\"form.remark\" placeholder=\"请输入备注\" />\n        </el-form-item>\n        <el-form-item label=\"拍摄日期\" prop=\"shootDate\">\n          <el-date-picker clearable\n            v-model=\"form.shootDate\"\n            type=\"date\"\n            value-format=\"yyyy-MM-dd\"\n            placeholder=\"请选择拍摄日期\">\n          </el-date-picker>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\n        <el-button @click=\"cancel\">取 消</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { listRuoyi-shoot, getRuoyi-shoot, delRuoyi-shoot, addRuoyi-shoot, updateRuoyi-shoot } from \"@/api/ruoyi-shoot/shoot/index\";\n\nexport default {\n  name: \"Ruoyi-shoot\",\n  data() {\n    return {\n      // 遮罩层\n      loading: true,\n      // 选中数组\n      ids: [],\n      // 非单个禁用\n      single: true,\n      // 非多个禁用\n      multiple: true,\n      // 显示搜索条件\n      showSearch: true,\n      // 总条数\n      total: 0,\n      // 拍摄管理表格数据\n      ruoyi-shootList: [],\n      // 弹出层标题\n      title: \"\",\n      // 是否显示弹出层\n      open: false,\n      // 查询参数\n      queryParams: {\n        pageNum: 1,\n        pageSize: 10,\n        titile: null,\n        shootName: null,\n        description: null,\n        shootType: null,\n        shootEquipment: null,\n        shootLocation: null,\n        isDelete: null,\n        shootDate: null,\n      },\n      // 表单参数\n      form: {},\n      // 表单校验\n      rules: {\n        isDelete: [\n          { required: true, message: \"是否删除不能为空\", trigger: \"blur\" }\n        ],\n      }\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    /** 查询拍摄管理列表 */\n    getList() {\n      this.loading = true;\n      listRuoyi-shoot(this.queryParams).then(response => {\n        this.ruoyi-shootList = response.rows;\n        this.total = response.total;\n        this.loading = false;\n      });\n    },\n    // 取消按钮\n    cancel() {\n      this.open = false;\n      this.reset();\n    },\n    // 表单重置\n    reset() {\n      this.form = {\n        photoId: null,\n        titile: null,\n        shootName: null,\n        description: null,\n        shootType: null,\n        shootEquipment: null,\n        shootLocation: null,\n        isDelete: null,\n        remark: null,\n        shootDate: null,\n        createTime: null,\n        updateTime: null\n      };\n      this.resetForm(\"form\");\n    },\n    /** 搜索按钮操作 */\n    handleQuery() {\n      this.queryParams.pageNum = 1;\n      this.getList();\n    },\n    /** 重置按钮操作 */\n    resetQuery() {\n      this.resetForm(\"queryForm\");\n      this.handleQuery();\n    },\n    // 多选框选中数据\n    handleSelectionChange(selection) {\n      this.ids = selection.map(item => item.photoId)\n      this.single = selection.length!==1\n      this.multiple = !selection.length\n    },\n    /** 新增按钮操作 */\n    handleAdd() {\n      this.reset();\n      this.open = true;\n      this.title = \"添加拍摄管理\";\n    },\n    /** 修改按钮操作 */\n    handleUpdate(row) {\n      this.reset();\n      const photoId = row.photoId || this.ids\n      getRuoyi-shoot(photoId).then(response => {\n        this.form = response.data;\n        this.open = true;\n        this.title = \"修改拍摄管理\";\n      });\n    },\n    /** 提交按钮 */\n    submitForm() {\n      this.$refs[\"form\"].validate(valid => {\n        if (valid) {\n          if (this.form.photoId != null) {\n            updateRuoyi-shoot(this.form).then(response => {\n              this.$modal.msgSuccess(\"修改成功\");\n              this.open = false;\n              this.getList();\n            });\n          } else {\n            addRuoyi-shoot(this.form).then(response => {\n              this.$modal.msgSuccess(\"新增成功\");\n              this.open = false;\n              this.getList();\n            });\n          }\n        }\n      });\n    },\n    /** 删除按钮操作 */\n    handleDelete(row) {\n      const photoIds = row.photoId || this.ids;\n      this.$modal.confirm('是否确认删除拍摄管理编号为\"' + photoIds + '\"的数据项？').then(function() {\n        return delRuoyi-shoot(photoIds);\n      }).then(() => {\n        this.getList();\n        this.$modal.msgSuccess(\"删除成功\");\n      }).catch(() => {});\n    },\n    /** 导出按钮操作 */\n    handleExport() {\n      this.download('shoot/ruoyi-shoot/export', {\n        ...this.queryParams\n      }, `ruoyi-shoot_${new Date().getTime()}.xlsx`)\n    }\n  }\n};\n</script>\n"]}]}