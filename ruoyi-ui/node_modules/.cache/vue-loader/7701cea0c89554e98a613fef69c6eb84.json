ndex.vue","sourceRoot":"src/views/shoot/ruoyi-shoot","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <el-form :model=\"queryParams\" ref=\"queryForm\" size=\"small\" :inline=\"true\" v-show=\"showSearch\" label-width=\"68px\">\r\n      <el-form-item label=\"主题\" prop=\"title\">\r\n        <el-input\r\n          v-model=\"queryParams.title\"\r\n          placeholder=\"请输入主题\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"拍摄名称\" prop=\"shootName\">\r\n        <el-input\r\n          v-model=\"queryParams.shootName\"\r\n          placeholder=\"请输入拍摄名称\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"描述\" prop=\"description\">\r\n        <el-input\r\n          v-model=\"queryParams.description\"\r\n          placeholder=\"请输入描述\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"拍摄设备\" prop=\"shootEquipment\">\r\n        <el-input\r\n          v-model=\"queryParams.shootEquipment\"\r\n          placeholder=\"请输入拍摄设备\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"拍摄地点\" prop=\"shootLocation\">\r\n        <el-input\r\n          v-model=\"queryParams.shootLocation\"\r\n          placeholder=\"请输入拍摄地点\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"是否删除\" prop=\"isDelete\">\r\n        <el-input\r\n          v-model=\"queryParams.isDelete\"\r\n          placeholder=\"请输入是否删除\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"拍摄日期\" prop=\"shootDate\">\r\n        <el-date-picker clearable\r\n          v-model=\"queryParams.shootDate\"\r\n          type=\"date\"\r\n          value-format=\"yyyy-MM-dd\"\r\n          placeholder=\"请选择拍摄日期\">\r\n        </el-date-picker>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"handleQuery\">搜索</el-button>\r\n        <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetQuery\">重置</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n    <el-row :gutter=\"10\" class=\"mb8\">\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"primary\"\r\n          plain\r\n          icon=\"el-icon-plus\"\r\n          size=\"mini\"\r\n          @click=\"handleAdd\"\r\n          v-hasPermi=\"['shoot:shoot:add']\"\r\n        >新增</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"success\"\r\n          plain\r\n          icon=\"el-icon-edit\"\r\n          size=\"mini\"\r\n          :disabled=\"single\"\r\n          @click=\"handleUpdate\"\r\n          v-hasPermi=\"['shoot:shoot:edit']\"\r\n        >修改</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"danger\"\r\n          plain\r\n          icon=\"el-icon-delete\"\r\n          size=\"mini\"\r\n          :disabled=\"multiple\"\r\n          @click=\"handleDelete\"\r\n          v-hasPermi=\"['shoot:shoot:remove']\"\r\n        >删除</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"warning\"\r\n          plain\r\n          icon=\"el-icon-download\"\r\n          size=\"mini\"\r\n          @click=\"handleExport\"\r\n          v-hasPermi=\"['shoot:shoot:export']\"\r\n        >导出</el-button>\r\n      </el-col>\r\n      <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar>\r\n    </el-row>\r\n\r\n    <el-table v-loading=\"loading\" :data=\"shootList\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\r\n      <el-table-column label=\"id\" align=\"center\" prop=\"photoId\" />\r\n      <el-table-column label=\"主题\" align=\"center\" prop=\"title\" />\r\n      <el-table-column label=\"拍摄名称\" align=\"center\" prop=\"shootName\" />\r\n      <el-table-column label=\"描述\" align=\"center\" prop=\"description\" />\r\n      <el-table-column label=\"拍摄类型\" align=\"center\" prop=\"shootType\" />\r\n      <el-table-column label=\"拍摄设备\" align=\"center\" prop=\"shootEquipment\" />\r\n      <el-table-column label=\"拍摄地点\" align=\"center\" prop=\"shootLocation\" />\r\n      <el-table-column label=\"是否删除\" align=\"center\" prop=\"isDelete\" />\r\n      <el-table-column label=\"备注\" align=\"center\" prop=\"remark\" />\r\n      <el-table-column label=\"拍摄日期\" align=\"center\" prop=\"shootDate\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ parseTime(scope.row.shootDate, '{y}-{m}-{d}') }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-edit\"\r\n            @click=\"handleUpdate(scope.row)\"\r\n            v-hasPermi=\"['shoot:shoot:edit']\"\r\n          >修改</el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-delete\"\r\n            @click=\"handleDelete(scope.row)\"\r\n            v-hasPermi=\"['shoot:shoot:remove']\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    \r\n    <pagination\r\n      v-show=\"total>0\"\r\n      :total=\"total\"\r\n      :page.sync=\"queryParams.pageNum\"\r\n      :limit.sync=\"queryParams.pageSize\"\r\n      @pagination=\"getList\"\r\n    />\r\n\r\n    <!-- 添加或修改拍摄管理对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item label=\"主题\" prop=\"title\">\r\n          <el-input v-model=\"form.title\" placeholder=\"请输入主题\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"拍摄名称\" prop=\"shootName\">\r\n          <el-input v-model=\"form.shootName\" placeholder=\"请输入拍摄名称\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"描述\" prop=\"description\">\r\n          <el-input v-model=\"form.description\" placeholder=\"请输入描述\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"拍摄设备\" prop=\"shootEquipment\">\r\n          <el-input v-model=\"form.shootEquipment\" placeholder=\"请输入拍摄设备\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"拍摄地点\" prop=\"shootLocation\">\r\n          <el-input v-model=\"form.shootLocation\" placeholder=\"请输入拍摄地点\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"是否删除\" prop=\"isDelete\">\r\n          <el-input v-model=\"form.isDelete\" placeholder=\"请输入是否删除\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"备注\" prop=\"remark\">\r\n          <el-input v-model=\"form.remark\" placeholder=\"请输入备注\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"拍摄日期\" prop=\"shootDate\">\r\n          <el-date-picker clearable\r\n            v-model=\"form.shootDate\"\r\n            type=\"date\"\r\n            value-format=\"yyyy-MM-dd\"\r\n            placeholder=\"请选择拍摄日期\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { listShoot, getShoot, delShoot, addShoot, updateShoot } from \"@/api/shoot/shoot\";\r\n\r\nexport default {\r\n  name: \"Shoot\",\r\n  data() {\r\n    return {\r\n      // 遮罩层\r\n      loading: true,\r\n      // 选中数组\r\n      ids: [],\r\n      // 非单个禁用\r\n      single: true,\r\n      // 非多个禁用\r\n      multiple: true,\r\n      // 显示搜索条件\r\n      showSearch: true,\r\n      // 总条数\r\n      total: 0,\r\n      // 拍摄管理表格数据\r\n      shootList: [],\r\n      // 弹出层标题\r\n      title: \"\",\r\n      // 是否显示弹出层\r\n      open: false,\r\n      // 查询参数\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        title: null,\r\n        shootName: null,\r\n        description: null,\r\n        shootType: null,\r\n        shootEquipment: null,\r\n        shootLocation: null,\r\n        isDelete: null,\r\n        shootDate: null,\r\n      },\r\n      // 表单参数\r\n      form: {},\r\n      // 表单校验\r\n      rules: {\r\n        isDelete: [\r\n          { required: true, message: \"是否删除不能为空\", trigger: \"blur\" }\r\n        ],\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n    /** 查询拍摄管理列表 */\r\n    getList() {\r\n      this.loading = true;\r\n      listShoot(this.queryParams).then(response => {\r\n        this.shootList = response.rows;\r\n        this.total = response.total;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.open = false;\r\n      this.reset();\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        photoId: null,\r\n        title: null,\r\n        shootName: null,\r\n        description: null,\r\n        shootType: null,\r\n        shootEquipment: null,\r\n        shootLocation: null,\r\n        isDelete: null,\r\n        remark: null,\r\n        shootDate: null,\r\n        createTime: null,\r\n        updateTime: null\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n    /** 搜索按钮操作 */\r\n    handleQuery() {\r\n      this.queryParams.pageNum = 1;\r\n      this.getList();\r\n    },\r\n    /** 重置按钮操作 */\r\n    resetQuery() {\r\n      this.resetForm(\"queryForm\");\r\n      this.handleQuery();\r\n    },\r\n    // 多选框选中数据\r\n    handleSelectionChange(selection) {\r\n      this.ids = selection.map(item => item.photoId)\r\n      this.single = selection.length!==1\r\n      this.multiple = !selection.length\r\n    },\r\n    /** 新增按钮操作 */\r\n    handleAdd() {\r\n      this.reset();\r\n      this.open = true;\r\n      this.title = \"添加拍摄管理\";\r\n    },\r\n    /** 修改按钮操作 */\r\n    handleUpdate(row) {\r\n      this.reset();\r\n      const photoId = row.photoId || this.ids\r\n      getShoot(photoId).then(response => {\r\n        this.form = response.data;\r\n        this.open = true;\r\n        this.title = \"修改拍摄管理\";\r\n      });\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate(valid => {\r\n        if (valid) {\r\n          if (this.form.photoId != null) {\r\n            updateShoot(this.form).then(response => {\r\n              this.$modal.msgSuccess(\"修改成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          } else {\r\n            addShoot(this.form).then(response => {\r\n              this.$modal.msgSuccess(\"新增成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    /** 删除按钮操作 */\r\n    handleDelete(row) {\r\n      const photoIds = row.photoId || this.ids;\r\n      this.$modal.confirm('是否确认删除拍摄管理编号为\"' + photoIds + '\"的数据项？').then(function() {\r\n        return delShoot(photoIds);\r\n      }).then(() => {\r\n        this.getList();\r\n        this.$modal.msgSuccess(\"删除成功\");\r\n      }).catch(() => {});\r\n    },\r\n    /** 导出按钮操作 */\r\n    handleExport() {\r\n      this.download('shoot/shoot/export', {\r\n        ...this.queryParams\r\n      }, `shoot_${new Date().getTime()}.xlsx`)\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}