lumn label=\"id\" align=\"center\" prop=\"photoId\" />\n      <el-table-column label=\"主题\" align=\"center\" prop=\"title\" />\n      <el-table-column label=\"拍摄名称\" align=\"center\" prop=\"shootName\" />\n      <el-table-column label=\"描述\" align=\"center\" prop=\"description\" />\n      <el-table-column label=\"拍摄类型\" align=\"center\" prop=\"shootType\" />\n      <el-table-column label=\"拍摄设备\" align=\"center\" prop=\"shootEquipment\" />\n      <el-table-column label=\"拍摄地点\" align=\"center\" prop=\"shootLocation\" />\n      <el-table-column label=\"是否删除\" align=\"center\" prop=\"isDelete\" />\n      <el-table-column label=\"备注\" align=\"center\" prop=\"remark\" />\n      <el-table-column label=\"拍摄日期\" align=\"center\" prop=\"shootDate\" width=\"180\">\n        <template slot-scope=\"scope\">\n          <span>{{ parseTime(scope.row.shootDate, '{y}-{m}-{d}') }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\n        <template slot-scope=\"scope\">\n          <el-button\n            size=\"mini\"\n            type=\"text\"\n            icon=\"el-icon-edit\"\n            @click=\"handleUpdate(scope.row)\"\n            v-hasPermi=\"['shoot:shoot:edit']\"\n          >修改</el-button>\n          <el-button\n            size=\"mini\"\n            type=\"text\"\n            icon=\"el-icon-delete\"\n            @click=\"handleDelete(scope.row)\"\n            v-hasPermi=\"['shoot:shoot:remove']\"\n          >删除</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination\n      v-show=\"total>0\"\n      :total=\"total\"\n      :page.sync=\"queryParams.pageNum\"\n      :limit.sync=\"queryParams.pageSize\"\n      @pagination=\"getList\"\n    />\n\n    <!-- 添加或修改拍摄管理对话框 -->\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\