   :page.sync=\"queryParams.pageNum\"\r\n      :limit.sync=\"queryParams.pageSize\"\r\n      @pagination=\"getList\"\r\n    />\r\n\r\n    <!-- 添加或修改拍摄管理对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item label=\"主题\" prop=\"title\">\r\n          <el-input v-model=\"form.title\" placeholder=\"请输入主题\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"拍摄名称\" prop=\"shootName\">\r\n          <el-input v-model=\"form.shootName\" placeholder=\"请输入拍摄名称\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"描述\" prop=\"description\">\r\n          <el-input v-model=\"form.description\" placeholder=\"请输入描述\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"拍摄设备\" prop=\"shootEquipment\">\r\n          <el-input v-model=\"form.shootEquipment\" placeholder=\"请输入拍摄设备\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"拍摄地点\" prop=\"shootLocation\">\r\n          <el-input v-model=\"form.shootLocation\" placeholder=\"请输入拍摄地点\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"是否删除\" prop=\"isDelete\">\r\n          <el-input v-model=\"form.isDelete\" placeholder=\"请输入是否删除\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"备注\" prop=\"remark\">\r\n          <el-input v-model=\"form.remark\" placeholder=\"请输入备注\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"拍摄日期\" prop=\"shootDate\">\r\n          <el-date-picker clearable\r\n            v-model=\"form.shootDate\"\r\n            type=\"date\"\r\n            value-format=\"yyyy-MM-dd\"\r\n            placeholder=\"请选择拍摄日期\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n